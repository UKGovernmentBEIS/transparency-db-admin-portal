                        <% } else { %>
                        <input class="govuk-input govuk-date-input__input govuk-input--width-2"
                          id="Audit_Granting_Date_Day_End" name="Audit_Granting_Date_Day_End" type="text"
                          <% if(ssn.Audit_Granting_Date_Day_End_Global) {%>
                          value="<%= ssn.Audit_Granting_Date_Day_End_Global%>" <% } %> />
                        <% } %>
                      </div>
                    </div>
                    <div class="govuk-date-input__item">
                      <div class="govuk-form-group">
                        <label class="govuk-label govuk-date-input__label" for="Audit_Granting_Date_Month_End">
                          Month
                        </label>

                        <% if( ssn.Audit_Granting_Date_Month_End_Error) { %>
                        <input class="govuk-input govuk-date-input__input govuk-input--width-2 govuk-input--error"
                          id="Audit_Granting_Date_Month_End" name="Audit_Granting_Date_Month_End" type="text"
                          pattern="[0-9]*" inputmode="numeric" <% if(ssn.Audit_Granting_Date_Month_End_Global) {%>
                          value="<%= ssn.Audit_Granting_Date_Month_End_Global%>" <% } %> />
                        <% } else { %>

                        <input class="govuk-input govuk-date-input__input govuk-input--width-2"
                          id="Audit_Granting_Date_Month_End" name="Audit_Granting_Date_Month_End" type="text"
                          pattern="[0-9]*" inputmode="numeric" <% if(ssn.Audit_Granting_Date_Month_End_Global) {%>
                          value="<%= ssn.Audit_Granting_Date_Month_End_Global%>" <% } %> />
                        <% } %>
                      </div>
                    </div>
                    <div class="govuk-date-input__item">
                      <div class="govuk-form-group">
                        <label class="govuk-label govuk-date-input__label" for="Audit_Granting_Date_Year_End">
                          Year
                        </label>
                        <% if( ssn.Audit_Granting_Date_Year_End_Error) { %>
                        <input class="govuk-input govuk-date-input__input govuk-input--width-4 govuk-input--error"
                          id="Audit_Granting_Date_Year_End" name="Audit_Granting_Date_Year_End" type="text"
                          pattern="[0-9]*" inputmode="numeric" <% if(ssn.Audit_Granting_Date_Year_End_Global) {%>
                          value="<%= ssn.Audit_Granting_Date_Year_End_Global%>" <% } %> />
                        <% } else { %>
                        <input class="govuk-input govuk-date-input__input govuk-input--width-4"
                          id="Audit_Granting_Date_Year_End" name="Audit_Granting_Date_Year_End" type="text"
                          pattern="[0-9]*" inputmode="numeric" <% if(ssn.Audit_Granting_Date_Year_End_Global) {%>
                          value="<%= ssn.Audit_Granting_Date_Year_End_Global%>" <% } %> />
                        <% } %>
                      </div>
                    </div>
                    <button class="govuk-button" data-module="govuk-button" id="date-search-button">
                      Search
                    </button>

                  </div>
                </fieldset>
              </div>

              <% } else { %>

              <div class="govuk-form-group">
                <fieldset class="govuk-fieldset" role="group" aria-describedby="Audit_Granting_Date-hint">
                  <legend class="govuk-fieldset__legend govuk-fieldset__legend--l">

                    Search to

                  </legend>

                  <div class="govuk-date-input" id="Audit_Granting_Date">
                    <div class="govuk-date-input__item">
                      <div class="govuk-form-group">
                        <label class="govuk-label govuk-date-input__label" for="Audit_Granting_Date_Day_End">
