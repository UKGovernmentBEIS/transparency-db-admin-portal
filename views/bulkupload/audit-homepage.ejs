      </div>

      <% if( !noresult) { %>
        <div class="govuk-grid-row">

          <!-- <form action="/homepage" method="POST"> -->
          <section class="govuk-table-responsive govuk-grid-column-full">
            <table class="govuk-table" id="searchresult-table">
              <caption class="govuk-table__caption">
                <h1 class="govuk-fieldset__heading"></h1>
              </caption>
              <thead class="govuk-table__head">
                <tr class="govuk-table__row">
                  <th scope="col" class="govuk-table__header app-custom-class govuk-!-width-one-quarter">
                    <a href="/auditsortroute/?page=audit_username" style="text-decoration: none">
                      User Name
                      <% if (ssn.audit_username_arrow  == "upanddown" ) { %>
                      <img id="audit_username_updownarrow" src="/assets/images/UpDownArrow.jpg"
                        alt="Currently not sorted">
                      <% } else if (ssn.audit_username_arrow  == "downdecending" ) { %>
                      <img id="audit_username_downarrow" src="/assets/images/DownArrow.jpg"
                        alt="Currently sorted by Descending order">
                      <% } else { %>
                      <img id="audit_username_uparrow" src="/assets/images/UpArrow.jpg"
                        alt="Currently sorted by Ascending order">
                      <% } %>
                    </a>
                  </th>

                  <th scope="col" class="govuk-table__header app-custom-class govuk-!-width-one-quarter">
                    <a href="/auditsortroute/?page=granting_authority" style="text-decoration: none">
