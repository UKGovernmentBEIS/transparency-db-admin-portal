                          value="<%= ssn.Audit_Granting_Date_Month_Start_Global%>" <% } %> />
                        <% } %>
                      </div>
                    </div>
                    <div class="govuk-date-input__item">
                      <div class="govuk-form-group">
                        <label class="govuk-label govuk-date-input__label" for="Audit_Granting_Date_Year_Start">
