<!DOCTYPE html>
<html lang="en">

<head>
  <title>GOV.UK - Audit search page</title>
  <%- include('../partials/headercss.ejs') %>
  <script>
    function formatDateToTwoDigits(input){
      if(input.value.length === 1){
        input.value = "0" + input.value;
      }
    }
  </script>
</head>


<body class="govuk-template__body app-example-page">
  <script>document.body.className = ((document.body.className) ? document.body.className + ' js-enabled' : 'js-enabled');</script>
  <a href="#main-content" class="govuk-skip-link">Skip to main content</a>
  <%- include('../partials/cookies-consent.ejs') %> <%- include('../partials/header.ejs') %>
  <div class="govuk-width-container ">
    <div class="govuk-phase-banner">
      <p class="govuk-phase-banner__content">
        <strong class="govuk-tag govuk-phase-banner__content__tag">
          beta
        </strong>
        <span class="govuk-phase-banner__text">
          This is a new service â€“ your <a class="govuk-link" href="/feedbackform">feedback</a> will help us to improve
          it.
        </span>
      </p>
    </div>
    <main id="main-content" role="main">
      
      <%- include('../partials/navigationbar.ejs') %>

      <div class="govuk-breadcrumbs ">
        <ol class="govuk-breadcrumbs__list">
          <li class="govuk-breadcrumbs__list-item">
            <a class="govuk-breadcrumbs__link" href="/">Dashboard</a>
          </li>
          <li class="govuk-breadcrumbs__list-item">
            <a class="govuk-breadcrumbs__link" aria-current="page" href="#">Audit log</a>
          </li>

        </ol>
      </div>
      <!-- bread crumbs ends here -->

      <!-- Body header starts here -->
      <h1 class="govuk-heading-xl">Audit log</h1>
      <!-- Body header ends here -->
      <% if(noresult) { %>
      <div class="govuk-error-summary" id="error-alert" aria-labelledby="error-summary-title" role="alert" tabindex="-1"
        data-module="govuk-error-summary">

        <div class="govuk-error-summary__body">
          <ul class="govuk-list govuk-error-summary__list">
            <li>
              <%= nodata %>
            </li>
          </ul>
        </div>
      </div>
      <% } %>
      <div class="govuk-grid-row">
        <% if( !noresult) { %>

        <div class="govuk-grid-column-full" id="audit_search_text">
          <label class="govuk-label">
            Below is a list of all activity within the system ordered by latest at the top. You can search and filter
            for a specific person or public authority.
          </label>
        </div>

        <% } else { %>
        <div class="govuk-grid-column-full" id="audit_search_text">
          <label class="govuk-label">
            You can search and filter for a specific person or public authority.
          </label>
        </div>
        <% } %>
        <div class="govuk-grid-column-one-half">
          <form action="/auditsearchbytextroute" method="post">
            <!-- <input class="govuk-input govuk-!-width-three-quarters" id="search_text" name="search_text" type="text">
             -->
            <input class="govuk-input govuk-!-width-three-quarters" id="search-text" name="search_text" type="text"
              aria-describedby="subsidy_search_text" <% if(ssn.audit_search_by_text) {%>
              value="<%= ssn.audit_search_by_text %>" <% } %> />

            <button class="govuk-button" data-module="govuk-button" id="search-button">
              Search
            </button>
          </form>
        </div>



        <!-- *****************************************  -->
        <!-- Get the Legal Granting Date     from user  -->
        <!-- *****************************************  -->
        <div class="govuk-grid-column-full govuk-padding-0">
          <form action="/auditsearchbydateroute" method="post">
            <div class="govuk-grid-column-one-third">
              <% if( ssn.Audit_Granting_Date_Day_Start_Error || ssn.Audit_Granting_Date_Month_Start_Error || ssn.Audit_Granting_Date_Year_Start_Error) { %>
              <div class="govuk-form-group govuk-form-group--error">
                <fieldset class="govuk-fieldset" role="group" aria-describedby="Audit_Granting_Date-hint">
                  <legend class="govuk-fieldset__legend govuk-fieldset__legend--l">
                    Search from
                  </legend>

                  <span class="govuk-error-message" id="Subsidy_Control_Number_Error">
                    <span class="govuk-visually-hidden">Error:</span> Enter the granting date
                  </span>

                  <div class="govuk-date-input" id="Audit_Granting_Date">
                    <div class="govuk-date-input__item">
                      <div class="govuk-form-group">
                        <label class="govuk-label govuk-date-input__label" for="Audit_Granting_Date_Day_Start">
                          Day
                        </label>
                        <% if( ssn.Audit_Granting_Date_Day_Start_Error) { %>
                        <input class="govuk-input govuk-date-input__input govuk-input--width-2 govuk-input--error"
                          id="Audit_Granting_Date_Day_Start" name="Audit_Granting_Date_Day_Start" type="text"
                          pattern="[0-9]*" inputmode="numeric" <% if(ssn.Audit_Granting_Date_Day_Start_Global) {%>
                          value="<%= ssn.Audit_Granting_Date_Day_Start_Global%>" <% } %> />
                        <% } else { %>
                        <input class="govuk-input govuk-date-input__input govuk-input--width-2"
                          id="Audit_Granting_Date_Day_Start" name="Audit_Granting_Date_Day_Start" type="text"
                          pattern="[0-9]*" inputmode="numeric" <% if(ssn.Audit_Granting_Date_Day_Start_Global) {%>
                          value="<%= ssn.Audit_Granting_Date_Day_Start_Global%>" <% } %> />
                        <% } %>
                      </div>
                    </div>
                    <div class="govuk-date-input__item">
                      <div class="govuk-form-group">
                        <label class="govuk-label govuk-date-input__label" for="Audit_Granting_Date_Month_Start">
                          Month
                        </label>

                        <% if( ssn.Audit_Granting_Date_Month_Start_Error) { %>
                        <input class="govuk-input govuk-date-input__input govuk-input--width-2 govuk-input--error"
                          id="Audit_Granting_Date_Month_Start" name="Audit_Granting_Date_Month_Start" type="text"
                          pattern="[0-9]*" inputmode="numeric" <% if(ssn.Audit_Granting_Date_Month_Start_Global) {%>
