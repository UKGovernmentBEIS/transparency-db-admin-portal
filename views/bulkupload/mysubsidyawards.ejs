      <form action="/collectcheckbox" method="GET" novalidate="">
        <section>
          <table class="govuk-table" id="datatable">
            <thead class="govuk-table__head">
              <tr class="govuk-table__row">
                <th scope="col" class="govuk-table__header">
                  <div class="govuk-checkboxes">
                    <div class="govuk-checkboxes__item">
                      <input class="govuk-checkboxes__input Subsidy_awardno_head" id="select_all" name="Subsidy_awardNo"
                        type="checkbox" value="Subsidy_awardNo">
                      <label class="govuk-label govuk-checkboxes__label_padding govuk-checkboxes__label "
                        for="Subsidy awardNo" id='Subsidy_awardno_lb'>
                        <b>Subsidy Name</b>
                      </label>
                    </div>
                  </div>
                </th>
                <th scope="col" class="govuk-table__header">Subsidy control no</th>
                <th scope="col" class="govuk-table__header">Granting authority</th>
                <th scope="col" class="govuk-table__header">Start date</th>
                <th scope="col" class="govuk-table__header">End date</th>
                <th scope="col" class="govuk-table__header govuk-table__header--numeric">Duration</th>
                <th scope="col" class="govuk-table__header govuk-table__header--numeric">Budget</th>
              </tr>
            </thead>
            <tbody class="govuk-table__body">
              <% awards.awards.forEach(function(item) { %>
              <tr>
                <td class="govuk-table__cell">
                  <div class="govuk-checkboxes">
                    <div class="govuk-checkboxes__item">
                      <input class="govuk-checkboxes__input checkbox Subsidy_awardNo" id="Subsidy_awardNo"
                        name="check[]" type="checkbox" value="Subsidy awardNo">
                      <label class="govuk-label govuk-checkboxes__label" for="Subsidy awardNo">
                        <%= item.SubsidyawardNo %>
                      </label>
                    </div>
                  </div>
                </td>
                <td class="govuk-table__cell"><%= item.Subsidymeasuretitle %></td>
                <td class="govuk-table__cell"><%= item.Status %></td>
                <td class="govuk-table__cell"><%= item.Beneficiaryname %></td>
                <td class="govuk-table__cell"><%= item.Grantingauthority %></td>
                <td class="govuk-table__cell govuk-table__cell--numeric"><%= item.Subsidyamount %></td>
                <td class="govuk-table__cell govuk-table__cell--numeric"><%= item.Subsidyamount %></td>
              </tr>
              <% } ); %>
            </tbody>
          </table>

