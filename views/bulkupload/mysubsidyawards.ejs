      <h1 class="govuk-heading-xl"> Subsidy awards</h1>
      <div class="govuk-grid-row">
        <div class="govuk-grid-column-full govuk-grid-row">
          <label class="govuk-label govuk-grid-column-full">
            You can search for specific subsidy awards by entering a subsidy award name, number, recipient or granting
            authority.
          </label>
        </div>

        <div class="govuk-grid-column-one-half ">
          <form class="govuk-!-width-full govuk-padding-0" action="/subsidyawardsearch" method="POST">

            <input class="govuk-input govuk-!-width-three-quarters" id="search_award_text" name="search_award_text"
              aria-label="User input text box for Searching Subsidy Award" type="text" <% if(Award_search_text) {%>
              value="<%= Award_search_text%>" <% } %> />
            <button class="govuk-button" data-module="govuk-button">
              Search
            </button>
          </form>
        </div>
        <div class="govuk-grid-column-one-third govuk-padding-0">
          <form class="govuk-!-width-full govuk-padding-0" action="/awardsfilterroute" method="GET">
            <select class="govuk-!-width-full govuk-select" name="sort" aria-describedby="status-message"
              aria-label="Drop down list that shows subsidy award by status" onchange="this.form.submit()">
              <% if(awards_status  == "") { %>
              <option disabled value="" selected>Filter results by status </option>
              <% } else { %>
              <option disabled value="">Filter results by status</option>
              <% }  %>
              <% if(awards_status  == "Awaiting Approval") { %>
              <option value="Awaiting Approval" selected>Awaiting Approval </option>
              <% } else { %>
              <option value="Awaiting Approval">Awaiting Approval</option>
              <% }  %>

              <% if(awards_status  == "Published") { %>
              <option value="Published" selected>Published </option>
              <% } else { %>
              <option value="Published">Published</option>
              <% }  %>

              <% if(awards_status  == "Deactive") { %>
              <option value="Deactive" selected>Deactive </option>
              <% } else { %>
              <option value="Deactive">Deactive</option>
              <% }  %>

              <% if(awards_status  == "Rejected") { %>
              <option value="Rejected" selected>Rejected </option>
              <% } else { %>
              <option value="Rejected">Rejected</option>
              <% }  %>

              <% if(awards_status  == "Show all") { %>
              <option value="Show all" selected>Show all </option>
              <% } else { %>
              <option value="Show all">Show all</option>
              <% }  %>

            </select>
          </form>
          <span id="status-message" style="display:none" aria-hidden="true">
            On selecting, the page will reload and display the subsidy awards.</span>
        </div>

        <div class="govuk-grid-column-full">
          <!-- <a href="/" style="color : black; text-decoration: none"> -->
          <button class="govuk-button" onclick="window.location.href='/addsubsidyaward'" data-module="govuk-button">
            Add a new subsidy award
          </button>
          <!-- </a> -->
          <!-- <a href=""> -->
          <button class="govuk-button" onclick="window.location.href='/bulkuploadsubsidy'" data-module="govuk-button">
            Add multiple subsidy awards
          </button>
          <!-- </a> -->
          <button class="govuk-button" data-module="govuk-button" disabled>
            Delete award
          </button>
          <div class="govuk-grid-column-one-quarter govuk-form-group govuk-display-flex govuk-padding-0"
            style="float:right;text-align: right;">
            <label>Results per page</label>
            <form class="govuk-padding-0 govuk-!-width-one-third" action="/awardspageperroute" method="GET">
              <select class="govuk-select" aria-label="Results per page shown in current page"
                aria-describedby="info-message" aria-autocomplete="list" name="sort" onchange="this.form.submit()">
                <% if(frontend_totalRecordsPerPage  == 10) { %>
                <option value="10" selected>10</option>
                <% } else { %>
                <option value="10">10</option>
                <% }  %>
                <% if(frontend_totalRecordsPerPage  == 20) { %>
                <option value="20" selected>20</option>
                <% } else { %>
                <option value="20">20</option>
                <% }  %>
                <% if(frontend_totalRecordsPerPage  == 50) { %>
                <option value="50" selected>50</option>
                <% } else { %>
                <option value="50">50</option>
                <% }  %>
                <% if(frontend_totalRecordsPerPage  == 100) { %>
                <option value="1000" selected>100</option>
                <% } else { %>
                <option value="100">100</option>
                <% }  %>
              </select>
            </form>
            <span id="info-message" style="display:none" aria-hidden="true">
              On selecting, the page will reload and display the subsidy awards.</span>
          </div>
        </div>
      </div>


      <form action="/collectcheckbox" method="GET" novalidate="">
        <section class="govuk-table-responsive">
          <table class="govuk-table" id="datatable">
