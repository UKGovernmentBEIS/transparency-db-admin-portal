      <div class="govuk-grid-row">
        <div class="govuk-grid-column-full">
          <!-- <form action="/filtersearch" method="POST" > -->
          <legend class="govuk-fieldset__legend" style="font-weight: bold;">Search</legend>
          <p class="label-2 label">
            Search for Granting Authority Name or ID
          </p>
          <div class="govuk-grid-column-three-quarters govuk-filter-input-position-div govuk-p-0">
            <div class="govuk-grid-column-one-half govuk-p-0">
              <form action="/mygrantingauthority" method="POST">
                <input class="govuk-input govuk-!-width-three-quarters" id="filter-input" name="grantingAuthorityName"
                  type="text" value="<%= grantingAuthorityName%>" autocomplete="off" />
                <!-- <span class="clear-all">
                  <img src="/assets/images/u1733.svg" alt="" />
                </span> -->
                <button class="govuk-button govuk-button--success" id="getFilterTags">
                  Search
                </button>
              </form>
            </div>

            <!-- <select class="govuk-select" name="sort" style="width: 100%">
                <option value="" disabled selected>Only show me...</option>
                <option value="">Draft</option>
                <option value="">Finalized</option>
                <option value="">Published</option>
                <option value="">Deactivated</option>
              </select> -->
            <form class="govuk-grid-column-one-half" action="/mygrantingauthority" method="POST">
              <select class="govuk-select" name="sort" style="width: 100%" onchange="this.form.submit()"
                aria-label="Results as per status shown in current page" aria-describedby="info-message"
                aria-autocomplete="list">
                <% if(sort == 'Active') { %>
                <option value="Active" selected>Active </option>
                <% } else {%>
                <option value="Active">Active </option>
                <% } %>
                <% if(sort == 'Inactive') { %>
                <option value="Inactive" selected>Inactive </option>
                <% } else {%>
                <option value="Inactive">Inactive </option>
                <% } %>
                <% if(sort == '') { %>
                <option value="" selected>Show all </option>
                <% } else {%>
                <option value="">Show all </option>
                <% } %>
                <!-- <option value="">Inactive </option>
                <option value="" selected>Show all</option> -->
              </select>
            </form>

          </div>
          <form action="/mygrantingauthority" method="POST" class="govuk-grid-column-one-quarter sorting-div govuk-p-0"
            style="text-align: right;">
            <label>Results per page</label>
            <select class="govuk-select" name="totalRecordsPerPage" onchange="this.form.submit()">
              <% if(frontend_totalRecordsPerPage == '10') { %>
              <option value="10" selected>10</option>
              <% } else {%>
              <option value="10">10</option>
              <% } %>
              <% if(frontend_totalRecordsPerPage == '20') { %>
              <option value="20" selected>20</option>
              <% } else {%>
              <option value="20">20</option>
              <% } %>
              <% if(frontend_totalRecordsPerPage == '50') { %>
              <option value="50" selected>50</option>
              <% } else {%>
              <option value="50">50</option>
              <% } %>
              <% if(frontend_totalRecordsPerPage == '100') { %>
              <option value="100" selected>100</option>
              <% } else {%>
              <option value="100">100</option>
              <% } %>
            </select>
          </form>
