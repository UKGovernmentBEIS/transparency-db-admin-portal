      <section class="govuk-table-responsive">
        <table class="govuk-table">
          <thead class="govuk-table__head">
            <tr class="govuk-table__row">
              <th scope="col" style="text-align: left" class="govuk-table__header app-custom-class">
                <p style="position: relative;margin: 0;">Granting Authority ID
                  <a href="#" style="float: right;">
                    <svg version="1.1" width="8px" height="8px" class="uparrow">
                      <g transform="matrix(1 0 0 1 0 -5 )">
                        <path d="M 0 13  L 4 5  L 8 13  L 0 13  Z " fill-rule="nonzero" fill="#1d70b8" stroke="none" />
                      </g>
                    </svg>

                    <svg version="1.1" class="downarrow" width="8px" height="8px">
                      <g transform="matrix(1 0 0 1 0 -5 )">
                        <path d="M 0 13  L 4 5  L 8 13  L 0 13  Z " fill-rule="nonzero" fill="#1d70b8" stroke="none" />
                      </g>
                    </svg>
                  </a>
                </p>
              </th>
              <th scope="col" class="govuk-table__header app-custom-class govuk-!-width-one-quarter" aria-sort="none">
                <p style="position: relative;margin: 0;"> Granting Authority name
                  <a href="#" style="float: right;">
                    <svg version="1.1" width="8px" height="8px" class="uparrow">
                      <g transform="matrix(1 0 0 1 0 -5 )">
                        <path d="M 0 13  L 4 5  L 8 13  L 0 13  Z " fill-rule="nonzero" fill="#1d70b8" stroke="none" />
                      </g>
                    </svg>
                    <svg version="1.1" class="downarrow" width="8px" height="8px">
                      <g transform="matrix(1 0 0 1 0 -5 )">
                        <path d="M 0 13  L 4 5  L 8 13  L 0 13  Z " fill-rule="nonzero" fill="#1d70b8" stroke="none" />
                      </g>
                    </svg>
                  </a>
                </p>
              </th>
              <th scope="col" class="govuk-table__header app-custom-class">
                Added by
              </th>
              <th scope="col" class="govuk-table__header app-custom-class">
                Status
              </th>
              <th scope="col" class="govuk-table__header app-custom-class">
                Created on
              </th>
              <th scope="col" class="govuk-table__header app-custom-class" aria-sort="none">
                Last modified
              </th>
            </tr>
          </thead>
          <tbody class="govuk-table__body">
            <% grantingAuthorityList.gaList.forEach(function(item) { %>
            <tr>
              <td style="text-align: center;" class="govuk-table__cell"><a
                  href="/editreviewgrantingauthority?gaid=<%=item.grantingAuthorityId %>&ganame=<%= item.grantingAuthorityName %>"><%=item.grantingAuthorityId %></a>
              </td>
              <td class="govuk-table__cell"><a
                  href="/editreviewgrantingauthority?gaid=<%=item.grantingAuthorityId %>&ganame=<%= item.grantingAuthorityName %>"><%= item.grantingAuthorityName %></a>
              </td>
              <td class="govuk-table__cell"><%= item.approvedBy %></td>
              <td class="govuk-table__cell"><%= item.status %></td>
              <!-- <td>13:23 - 11 November 2020</td>
              <td>11 November 2020</td> -->
              <td class="govuk-table__cell">
                <%= item.createdTimestamp %></td>
              <td class="govuk-table__cell"><%= item.lastModifiedTimestamp %></td>
            </tr>
            <% }) %>
          </tbody>
        </table>
      </section>
