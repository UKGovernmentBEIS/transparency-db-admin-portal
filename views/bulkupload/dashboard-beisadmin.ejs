  <script src="/assets/javascripts/vendor/modernizr.js"></script>

  <!-- <%- include('../partials/headersubsidy.ejs') %> -->
  <%- include('../partials/header.ejs') %>
  <link href="/assets/stylesheet/style.css" rel="stylesheet" type="text/css" />
  <div class="govuk-width-container">
    <main class="govuk-main-wrapper" id="main-content" role="main">
      <div class="govuk-grid-row">
        <div class="govuk-grid-column-full">
          <nav aria-label="Sub navigation">
            <ul class="govuk-sub-navigation__list">
              <li class="govuk-sub-navigation__item">
                <a
                  class="govuk-sub-navigation__link"
                  href="/mygrantingauthority"
                >
                  Granting Authorities
                </a>
              </li>

              <li class="govuk-sub-navigation__item">
                <a class="govuk-sub-navigation__link" href="#">
                  Users
                </a>
              </li>
              <li class="govuk-sub-navigation__item">
                <a class="govuk-sub-navigation__link" href="/mysubsidymeasures">
                  Subsidy Measures
                </a>
              </li>
              <li class="govuk-sub-navigation__item">
                <a class="govuk-sub-navigation__link" href="/mysubsidyawards">
                  Subsidy Awards
                </a>
              </li>

            
              <li class="govuk-sub-navigation__item">
                <a class="govuk-sub-navigation__link" href="#"> Audit log </a>
              </li>

              <li class="govuk-sub-navigation__item head-right">
                <a class="govuk-sub-navigation__link" href="/logintransparency">
                  Sign out
                </a>
              </li>
              <li class="govuk-sub-navigation__item head-right">
                <a class="govuk-sub-navigation__link" href="/logintransparency">
                  My account
                </a>
              </li>
            </ul>
          </nav>
        </div>
      </div>

      <div class="govuk-beis-heading">
        <legend class="form-group">
          <h2>Dashboard</h2>
        </legend>
        <legend
          class="govuk-fieldset__legend govuk-fieldset__legend--l form-group"
        >
          <h1 class="govuk-fieldset__heading">Welcome <%= dashboard_user_name %></h1>
        </legend>
      </div>
      <div class="govuk-border-top">
        <legend
          class="govuk-fieldset__legend govuk-fieldset__legend--l form-group"
        >
          <h2 class="govuk-fieldset_sub-heading">Granting Authority</h2>
        </legend>
        <div class="govuk-grid-row">
          <div
            class="govuk-grid-column-one-quarter govuk-charts govuk-form-group"
          >
            <label>All granting authorities</label>
            <p>Total granting authorities</p>
            <legend
              class="govuk-fieldset__legend govuk-fieldset__legend--l govuk-total-count">
              <%= dashboardawards.grantingAuthorityUserActionCount.totalGrantingAuthority %>
            </legend>
          </div>
          <div
            class="govuk-grid-column-one-quarter govuk-charts govuk-form-group"
          >
            <label>Published by me</label>
            <p>Total granting authorities I've published</p>
            <legend
              class="govuk-fieldset__legend govuk-fieldset__legend--l govuk-total-count"
            >
            <%= dashboardawards.grantingAuthorityUserActionCount.totalGAPublishedByUser %>
            </legend>
          </div>
          <div
            class="govuk-grid-column-one-quarter govuk-charts govuk-form-group"
          >
            <label>Deactive</label>
            <p>Total granting authorities that are not active</p>
            <legend
              class="govuk-fieldset__legend govuk-fieldset__legend--l govuk-total-count"
            >
            <%= dashboardawards.grantingAuthorityUserActionCount.totalDeactiveGA %>
            </legend>
          </div>
          <div
            class="govuk-grid-column-one-quarter govuk-charts govuk-form-group"
          >
            <label>Not published</label>
            <p>Number of granting authorities that are not published</p>
            <legend
              class="govuk-fieldset__legend govuk-fieldset__legend--l govuk-total-count"
            >
            <%= dashboardawards.grantingAuthorityUserActionCount.totalNotPublishedGA %>
            </legend>
          </div>
        </div>
        <div class="govuk-addGA form-group">
          <button class="govuk-button govuk-button--success" id="getFilterTags">
            Manage granting authorities
          </button>
          <button class="govuk-button govuk-button--success" id="getFilterTags">
            Add granting authority
          </button>
        </div>
        <table class="govuk-table">
          <caption class="govuk-table__caption govuk-GA-heading">
            My 5 last updated granting authorities
          </caption>
          <thead class="govuk-table__head">
            <tr class="govuk-table__row">
              <th
                scope="col"
                class="govuk-table__header govuk-!-width-one-half"
              >
                Granting Authority name
              </th>
              <th
                scope="col"
                class="govuk-table__header govuk-!-width-one-quarter"
              >
                Status
              </th>
              <th
                scope="col"
                class="govuk-table__header govuk-!-width-one-quarter"
              >
                Last modified
              </th>
            </tr>
          </thead>
          <tbody class="govuk-table__body">
            <% dashboardawards.grantingAuthorityResponse.forEach(function(item) { %>
              <tr>
                <td class="govuk-table__cell"> <a> <%= item.gaName %> </a> </td>
                <td class="govuk-table__cell"><%= item.status %></td>
                <td class="govuk-table__cell"> <%= item.lastModifiedDate  %>  </td>
              
              </tr>

              <% } ); %>
          
          </tbody>
        </table>
      </div>
      <div class="govuk-border-top">
        <legend
          class="govuk-fieldset__legend govuk-fieldset__legend--l form-group"
        >
          <h2 class="govuk-fieldset_sub-heading">Users</h2>
        </legend>
        <div class="govuk-grid-row">
          <div
            class="govuk-grid-column-one-quarter govuk-charts govuk-form-group"
          >
            <label>All users</label>
            <p>All users in the system</p>
            <legend
              class="govuk-fieldset__legend govuk-fieldset__legend--l govuk-total-count"
            >
              72
            </legend>
          </div>
          <div
            class="govuk-grid-column-one-quarter govuk-charts govuk-form-group"
          >
            <label>My users</label>
            <p>All the users I'm resposnible for</p>
            <legend
              class="govuk-fieldset__legend govuk-fieldset__legend--l govuk-total-count"
            >
              72
            </legend>
          </div>
          <div
            class="govuk-grid-column-one-quarter govuk-charts govuk-form-group"
          >
            <label>All new users</label>
            <p>All my new users</p>
            <legend
              class="govuk-fieldset__legend govuk-fieldset__legend--l govuk-total-count"
            >
              5
            </legend>
          </div>
          <div
            class="govuk-grid-column-one-quarter govuk-charts govuk-form-group"
          >
            <label>My new users</label>
            <p>My new users only</p>
            <legend
              class="govuk-fieldset__legend govuk-fieldset__legend--l govuk-total-count"
            >
              2
            </legend>
          </div>
        </div>
        <div class="govuk-addGA form-group">
          <button class="govuk-button govuk-button--success" id="getFilterTags">
            Manager Users
          </button>
          <button
            class="govuk-button govuk-button--success btn-md"
            id="getFilterTags"
          >
            Add User
          </button>
        </div>
        <table class="govuk-table">
          <caption class="govuk-table__caption govuk-GA-heading">
            My 5 last updated granting authorities by users
          </caption>
          <thead class="govuk-table__head">
            <tr class="govuk-table__row">
              <th scope="col" class="govuk-table__header">User name</th>
              <th scope="col" class="govuk-table__header">
                Granting Authority name
              </th>
              <th scope="col" class="govuk-table__header">Role</th>
              <th scope="col" class="govuk-table__header">Created on</th>
            </tr>
          </thead>
          <tbody class="govuk-table__body">
            <tr class="govuk-table__row">
              <td class="govuk-table__cell">Rachael Cole</td>
              <td scope="row" class="govuk-table__cell">
                <a href="" class="govuk-editGA">Adur District Council</a>
              </td>
              <td class="govuk-table__cell">Granting Authority Admin</td>
              <td class="govuk-table__cell">12 :34 - 11 November 2020</td>
            </tr>
            <tr class="govuk-table__row">
              <td class="govuk-table__cell">Claire Smith</td>
              <td scope="row" class="govuk-table__cell">
                <a href="" class="govuk-editGA"
