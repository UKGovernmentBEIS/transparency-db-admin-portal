            <div class="govuk-background">
              <legend class="govuk-fieldset__legend govuk-fieldset__legend--l govuk-total-count">
                <%= dashboardawards.grantingAuthorityUserActionCount.totalGrantingAuthority %>
              </legend>
              <p class="govuk-body govuk-!-font-weight-bold govuk-!-font-size-24 govuk-description-text">Total granting
                authorities</p>
            </div>
          </div>
          <div class="govuk-grid-column-one-third govuk-form-group">
            <div class="govuk-background">

              <legend class="govuk-fieldset__legend govuk-fieldset__legend--l govuk-total-count">
                <%= dashboardawards.grantingAuthorityUserActionCount.totalActiveGA %>
              </legend>
              <p class="govuk-body govuk-!-font-weight-bold govuk-!-font-size-24 govuk-description-text">Active</p>
            </div>
          </div>
          <div class="govuk-grid-column-one-third govuk-form-group">
            <div class="govuk-background">

              <legend class="govuk-fieldset__legend govuk-fieldset__legend--l govuk-total-count">
