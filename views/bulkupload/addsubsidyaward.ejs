<!DOCTYPE html>
<html lang="en">

<head>
  <title>GOV.UK - Add Subsidy Award page</title>
  <%- include('../partials/headercss.ejs') %>
  <!-- Global site tag (gtag.js) - Google Analytics -->
  <script async src="https://www.googletagmanager.com/gtag/js?id=UA-145652997-21"></script>

  <script src="assets/javascripts/jquery.min.js"></script>
  <script src="/assets/javascripts/all.js"></script>
  <script>
    $(document).ready(function () {
      if(document.getElementById('Standalone_Award_Yes').checked){
        document.getElementById('related-subsidy-scheme').style.display = 'none';
        document.getElementById('related-admin-program').style.display = 'none';
        document.getElementById('authority-url-and-description-container').style.display = 'block';
        document.getElementById('standalone-award-interest-container').style.display = 'block';
      }
      else
      {
        document.getElementById('authority-url-and-description-container').style.display = 'none';
      }
      jQuery('#Subsidy_Control_Number').on('input', function () {
        var SubsidyMeasureTitle = document.getElementById("Subsidy_Measure_Title");
        SubsidyMeasureTitle = $('#Subsidy_Control_Number').val();

      });

      const nationalCheckbox = document.getElementById("Spending_Region_National")

      if (nationalCheckbox.checked){
        selectNational(nationalCheckbox);
      }
      

      $('.govuk-date-input__input').on('keypress', function (e) {
        return e.metaKey || // cmd/ctrl
          e.which <= 0 || // arrow keys
          e.which == 8 || // delete key
          /[0-9]/.test(String.fromCharCode(e.which)); // numbers

      });


      document.getElementById('mylink').onclick = function () {
        document.getElementById('myform').submit();
        return false;
      };

    });

    changeSubsidyPurpose = (event) => {

      if (event.target.value == 'Other')
        document.getElementById("other_purpose").style.display = "block";
      else
        document.getElementById("other_purpose").style.display = "none";
    };

    getSubsidyInstrument = (subsidyValue, purposeValue, errorArray) => {
      if (errorArray > 0) {
        document.getElementById("error-alert").focus();
      }
      if (subsidyValue == 'Tax measures (tax credit, or tax/duty exemption)') {
        $('.amount_subsidy_tax').show();
        $('.amount_subsidy_nottax').hide();
      }
      else {
        $('.amount_subsidy_tax').hide();
        $('.amount_subsidy_nottax').show();
      }
      if (subsidyValue == 'Other')
        document.getElementById("other_type").style.display = "block";
      else
        document.getElementById("other_type").style.display = "none";

      if (purposeValue == 'Other')
        document.getElementById("other_purpose").style.display = "block";
      else
        document.getElementById("other_purpose").style.display = "none";
    }

    changeSubsidyType = (event) => {

      if (event.target.value == 'Tax measures (tax credit, or tax/duty exemption)') {
        $('.amount_subsidy_tax').show();
        $('.amount_subsidy_nottax').hide();
      }
      else {
        $('.amount_subsidy_tax').hide();
        $('.amount_subsidy_nottax').show();
      }
      if (event.target.value == 'Other')
        document.getElementById("other_type").style.display = "block";
      else
        document.getElementById("other_type").style.display = "none";
    }

    standaloneAwardToggle = (event) => {
      if(event.target.value == 'Yes'){
        document.getElementById('related-subsidy-scheme').style.display = 'none';
        document.getElementById('related-admin-program').style.display = 'none';
        document.getElementById('authority-url-and-description-container').style.display = 'block';
        document.getElementById('standalone-award-interest-container').style.display = 'block';
      }else{
        document.getElementById('related-subsidy-scheme').style.display = 'block';
        document.getElementById('related-admin-program').style.display = 'block';
        document.getElementById('authority-url-and-description-container').style.display = 'none';
        document.getElementById('standalone-award-interest-container').style.display = 'none';
      }

    };
    function selectAll(source) {
      const checkboxes = document.getElementsByClassName("govuk-checkboxes__input_spendingregion");
      for(var i=0, n=checkboxes.length;i<n;i++) {
        checkboxes[i].checked = source.checked;
        checkboxes[i].ariaChecked = source.checked;
      }
    }

    function selectNational(source) {
      var checkboxes = document.getElementsByClassName("govuk-checkboxes__input_spendingregion");
      checkboxes = Array.from(checkboxes).concat(document.getElementById("Spending_Region_Select_All"));
      for(var i=0, n=checkboxes.length;i<n;i++) {
        checkboxes[i].checked = false;
        checkboxes[i].ariaChecked = false;
        checkboxes[i].disabled = source.checked;
        checkboxes[i].ariaDisabled = source.checked;
      }
    }
  </script>
</head>

